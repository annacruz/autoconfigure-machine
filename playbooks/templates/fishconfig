set -g -x GOPATH /Users/yuizinha/go

set -g -x PATH /usr/local/bin $PATH
set -g -x PATH $GOPATH/bin $PATH

set -g -x MPW_FULLNAME 'yuizinha'

set -g -x GPG_TTY tty

test -s $HOME/.nvm-fish/nvm.fish; and source $HOME/.nvm-fish/nvm.fish

alias vim="vim -u ~/vim-config/vimrc"
alias mpw="mpw -t p"

eval (direnv hook fish)

set -e SSH_AGENT_PID

set -q gnupg_SSH_AUTH_SOCK_by; or set gnupg_SSH_AUTH_SOCK_by 0

if test $gnupg_SSH_AUTH_SOCK_by -ne %self
 set -g -x SSH_AUTH_SOCK (gpgconf --list-dirs agent-ssh-socket)
end
